cfgname = @doxfile@
doxygen = @doxygen@
srcdir  = @srcdir@

dox-clean-local:
	rm -rf -- doc-*/ html/ latex/

dox-all: $(cfgname)
	$(doxygen) "$<"

$(cfgname): gen-doxcfg.sh
	${SHELL} "$<" "$@"
